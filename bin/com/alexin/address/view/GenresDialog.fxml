<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import org.controlsfx.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" stylesheets="@GenresDialog.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.alexin.address.view.GenresDialogController">
   <center>
      <CheckListView fx:id="checkListView" style="-fx-background-color: #1d1d1d;" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" spacing="10.0" style="-fx-background-color: #1d1d1d;">
         <children>
            <Button mnemonicParsing="false" onAction="#handleConfirmed" text="Confirm" BorderPane.alignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
